{
    "name": "discord-music-fast",
    "version": "1.0.0",
    "private": true,
    "github": "https://github.com/BastosC/MusicFast-Discord-Extension",
    "dependencies": {
        "jszip-sync": "3.2.1-sync",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scripts": "^5.0.1",
        "sass": "^1.62.0",
        "usehooks-ts": "^2.14.0"
    },
    "scripts": {
        "start": "next start",
        "dev": "react-scripts start --no-inline-scripts",
